{
  "$schema": "http://json.schemastore.org/template",
  "author": "SummerFleur",
  "classifications": [ "Common", "Library", "StardewValley" ],
  "identity": "SummerFleur.SDVModTemplate",
  "groupIdentity": "SummerFleur.SDVModTemplate",
  "version": "1.0.0",
  "name": "SummerFleur Stardew Valley Mod Template",
  "description": "A template for creating Stardew Valley mods",
  "shortName": "summerfleur-sdvmod",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "ModName",
  "preferNameDirectory": true,
  "precedence": "100",
  "primaryOutputs": [
    {
      "path": "ModName.csproj"
    }
  ],
  "symbols": {
    "Framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": ".NET 6.0"
        }
      ],
      "defaultValue": "net6.0",
      "description": "The target framework for the project."
    }
  }
}